
@model IEnumerable<Final_Project.Models.Movy>
@{
    ViewBag.Title = "Admin_View_Movies";
}

<h2>Admin_View_Movies</h2>

<table>
    <tr>
        <td>
            M_id
        </td>
        <td>
            Movie Name
        </td>

    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.M_Id</td>
            <td>
            <td><a href="@s.Movie" id="btn" @*data-id="@s.M_Id" data-toggle="modal" data-target=".bs-example-modal-sm" class="my"*@>@s.M_Id</a></td>


            <td>
                <a href="/adminNew/DeleteMovie/@s.M_Id">Delete</a>
                @*<a href="/adminNew/UpdateImage/@s.M_id">Update</a>*@
            </td>
        </tr>
    }
</table>
