
@model IEnumerable<Final_Project.Models.English>
@{
    ViewBag.Title = "Admin_View_Movies";
}

<h2>Admin_View_Englisg</h2>

<table>
    <tr>
        <td>
            E_id
        </td>
        <td>
             Name
        </td>

    </tr>

    @foreach (var s in Model)
    {
        <tr>
            <td>@s.E_id</td>
            <td>
            <td><a href="@s.E_file" id="btn" @*data-id="@s.M_Id" data-toggle="modal" data-target=".bs-example-modal-sm" class="my"*@>@s.E_filename</a></td>


            <td>
                <a href="/adminNew/DeleteEng/@s.E_id">Delete</a>
                @*<a href="/adminNew/UpdateImage/@s.M_id">Update</a>*@
            </td>
        </tr>
    }
</table>
